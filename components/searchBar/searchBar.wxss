/* components/searchBar/searchBar.wxss */

.searchBar {
  height: 87rpx;
  background: #fff;
  padding: 14rpx 23rpx;
}

.searchBarBox {
  background: #f5f5f5;
  height: 72rpx;
  border-radius: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.inputBox {
  text-align: center;
  flex-grow: 1;
}

.icon {
  width: 30rpx;
  height: 30rpx;
  border: 1px solid #ccc;
  margin-left: 30rpx;
}

.icon img {
  width: 100%;
  height: 100%;
}

