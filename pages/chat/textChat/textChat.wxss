.chatMain {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  position: absolute;
}

.chatScrollView {
  flex-grow: 1;
  position: relative;
}

.chatScroll {
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
}

.msgList {
  margin-bottom: 40rpx;
}

.inputBox {
  font-size: 32rpx;
}
